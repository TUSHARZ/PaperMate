<?xml version="1.0" encoding="utf-8"?>
    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:id="@android:id/background">
            <shape>
                <gradient
                    android:startColor="#94efed"
                    android:endColor="#d9eaf2"
                    android:angle="-90" />
                <size
                    android:height="9dp"
                    android:width="9dp" />
                <corners android:radius="7dp" />
            </shape>
        </item>
        <item android:id="@android:id/progress">
            <clip>
                <shape>
                    <gradient
                        android:startColor="#328cdf"
                        android:endColor="#7fb8ed"
                        android:angle="-90" />
                    <corners android:radius="7dp" />
                </shape>
            </clip>
        </item>
    </layer-list>
